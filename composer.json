{
    "name": "brnbio/laravel-mysql-spatial",
    "description": "MySQL spatial data types extension for latest Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Joseph Estefane",
            "email": "estefanejoe@gmail.com"
        },
        {
            "name": "Frank Heider",
            "email": "heider669@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/database": "^9.0|^10.0",
        "geo-io/wkb-parser": "^1.0",
        "jmikola/geojson": "^1.0",
        "doctrine/dbal": "^3.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4",
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Brnbio\\LaravelMysqlSpatial\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Brnbio\\LaravelMysqlSpatial\\SpatialServiceProvider"
            ]
        }
    }
}
